#include <stdio.h>

int main() 
{
    int n, num, max;
    printf("Enter the number of integers: ");
    scanf("%d", &n);
    printf("Enter an integer: ");
    scanf("%d", &max);
    for(int i = 1; i < n; i++) 
	{
        printf("Enter an integer: ");
        scanf("%d", &num);
        if(num > max) 
		{
            max = num;
        }
    }
    printf("The biggest number is: %d\n", max);
    return 0;
}
